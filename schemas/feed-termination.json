{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://feedme.global/schemas/0.1/feed-termination#",
  "type": "object",
  "required": ["MessageType", "FeedName", "FeedArgs", "ErrorCode", "ErrorData"],
  "additionalProperties": false,
  "properties": {
    "MessageType": { "const": "FeedTermination" },
    "FeedName": { "$ref": "feed-name" },
    "FeedArgs": { "$ref": "feed-args" },
    "ErrorCode": { "$ref": "error-code" },
    "ErrorData": { "$ref": "error-data" }
  }
}
